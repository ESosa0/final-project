{
  "private": true,
  "dependencies": {
    "@ludois/react-scrollable-anchor": "^0.6.4",
    "@rails/webpacker": "^3.2.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-jest": "^23.4.2",
    "babel-preset-es2015": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
